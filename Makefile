CORE_SRC = esp32-hal-adc.c esp32-hal-dac.c esp32-hal-cpu.c esp32-hal-bt.c esp32-hal-gpio.c esp32-hal-i2c-slave.c esp32-hal-i2c.c esp32-hal-ledc.c esp32-hal-matrix.c esp32-hal-misc.c esp32-hal-psram.c esp32-hal-rmt.c esp32-hal-sigmadelta.c esp32-hal-time.c esp32-hal-spi.c esp32-hal-timer.c esp32-hal-tinyusb.c esp32-hal-touch.c esp32-hal-uart.c firmware_msc_fat.c stdlib_noniso.c wiring_pulse.c wiring_shift.c libb64/cdecode.c libb64/cencode.c Esp.cpp FirmwareMSC.cpp FunctionalInterrupt.cpp HWCDC.cpp HardwareSerial.cpp IPAddress.cpp IPv6Address.cpp MD5Builder.cpp Print.cpp Stream.cpp StreamString.cpp USB.cpp USBCDC.cpp USBMSC.cpp WMath.cpp WString.cpp base64.cpp cbuf.cpp main.cpp
CORE_SRC_CC = $(filter %.c, $(CORE_SRC))
CORE_SRC_CXX = $(filter %.cpp, $(CORE_SRC))



# Standard variables, composed from our scraped variables
#
CPPFLAGS        = $(SOMEDEFINES) $(MOREDEFINES) $(ANOTHERDEFINE)
CC              = $(GCC_TOOLCHAIN)/bin/xtensa-esp32-elf-gcc
CFLAGS          = $(LOTSAINCLUDES) $(MOREINCLUDES) $(OTHERFLAGS) $(FIXFLAGS)

CXX             = $(GCC_TOOLCHAIN)/bin/xtensa-esp32-elf-g++
CXXFLAGS        = $(LOTSAINCLUDES) $(MOREINCLUDES) $(SOMEFLAGS) $(FIXFLAGS)

LDFLAGS = $(LDOPTIONS) $(LDPATH) $(LINKERSCRIPTS) $(LDPRETEND_IS_UNDEFINED)
LDLIBS	= $(STATICLIBS)


# Rules
#
# Compile ESP32 "core" .c files into $(BUILD) .c.o files
$(patsubst %, $(BUILD)/%.o, $(CORE_SRC_CC)): $(BUILD)/%.o: $(SDK)/cores/esp32/%
	@mkdir -p $(@D)
	@$(CC) $(CPPFLAGS) $(CFLAGS) -c $< -o $@
	@echo '$$(CC) $(subst $(HOME),~,$<)'

# Compile ESP32 "core" .cpp files into $(BUILD) .cpp.o files
$(patsubst %, $(BUILD)/%.o, $(CORE_SRC_CXX)): $(BUILD)/%.o: $(SDK)/cores/esp32/%
	@mkdir -p $(@D)
	@$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $< -o $@
	@echo '$$(CXX) $(subst $(HOME),~,$<)'

# How to make the archive
$(BUILD)/core.a: $(patsubst %, $(BUILD)/%.o, $(CORE_SRC))
	@mkdir -p $(@D)
	$(AR) cr $@ $^

# Tweak implicit recipe for echoing
$(BUILD)/%.o: %.cpp
	@mkdir -p $(@D)
	@$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $< -o $@
	@echo '$$(CXX) -c $< -o $@'

$(BUILD)/%.elf: $(BUILD)/%.o $(BUILD)/core.a
	@$(CXX) $(LDFLAGS) -o $@ $^ $(LDLIBS)
	@$(GCC_TOOLCHAIN)/bin/xtensa-esp32-elf-size -A $@
	@echo '$$(LINK) -o $@ $^'

$(BUILD)/%.bin: $(BUILD)/%.elf
	$(ESPTOOL) --chip esp32 elf2image --flash_mode dio --flash_freq 80m --flash_size 4MB -o $@ $^

$(BUILD)/%.partitions.bin: partitions.csv
	python $(SDK)/tools/gen_esp32part.py -q $< $@

flash-%: $(BUILD)/%.bin $(BUILD)/%.partitions.bin
	$(ESPTOOL) --chip esp32                                       \
            --port /dev/ttyUSB0 --baud 921600                         \
            --before default_reset --after hard_reset write_flash     \
            -z                                                        \
            --flash_mode dio --flash_freq 80m --flash_size 4MB        \
            0xe000  $(SDK)/tools/partitions/boot_app0.bin             \
            0x1000  $(BOOTLOADER)                                     \
            0x8000  $(BUILD)/$*.partitions.bin                        \
            0x10000 $(BUILD)/$*.bin
